+++ ptxdist-0.7.5-kub/projects/innokom/etc/init.d/network-ppp	Fri Jul 29 16:14:36 2005
@@ -0,0 +1,31 @@
+#!/bin/sh
+#
+# /etc/init.d/network-ppp
+#
+# Copyright (C) 2003 Auerswald GmbH & Co. KG
+# Kai-Uwe Bloem <linux-development@auerswald.de>
+#
+
+echo -n "starting ppp... "
+
+if lsmod | grep n_hdlc >/dev/null; then
+	echo -n "[modules already loaded] "
+else
+	insmod slhc
+	insmod ppp_generic
+	insmod ppp_synctty
+	insmod n_hdlc
+fi
+
+# map http traffic such that for PPP port 80 works as well
+if [ -e /data/etc/eth.conf -a -e /data/etc/ppp.conf ]; then
+	ipaddr=`grep '^local-ip-address:' /data/etc/ppp.conf |
+		sed 's/^local-ip-address: *//g'`
+	port=`grep '^http-port:' /data/etc/eth.conf | sed 's/^http-port: *//g'`
+fi
+if [ -n "$ipaddr" -a -n "$port" -a "$port" -ne 80 ]; then
+	/usr/sbin/iptables -t nat -A PREROUTING -d $ipaddr -p tcp --dport 80 \
+		-j DNAT --to-destination $ipaddr:$port
+fi
+
+echo "done."
