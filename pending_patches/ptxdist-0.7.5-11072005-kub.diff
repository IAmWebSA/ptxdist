+#!/bin/sh
+#
+# /etc/init.d/read-hwclock
+#
+# Copyright (C) 2003 Auerswald GmbH & Co. KG
+# Kai-Uwe Bloem <linux-development@auerswald.de>
+#
+
+echo -n "reading the hardware clock... "
+
+if [ "`uname -r | cut -c1-3`" = "2.4" ]; then
+	kext=o
+else
+	kext=ko
+fi
+
+if [ ! -e /opt/auerswald/modules/auerrtc.$kext ]; then
+	echo "no driver found, setting default date"
+	date 050100002001
+	exit 0
+fi
+
+echo
+/sbin/insmod /opt/auerswald/modules/auerrtc.$kext
+/sbin/hwclock -s -u
+
+if [ "_`date | sed 's/^.* //g' | cut -c1`" != "_2" ]; then
+	echo "date is invalid, setting default date"
+	date 050100002001
+	hwclock -w -u
+else
+	date
+fi
