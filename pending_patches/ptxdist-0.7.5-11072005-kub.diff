++		"size\tused\tdirty\twasted\tfree\tunchecked\tobsolete\tdirent\tinode\tother\tcreated\tobsoleted\twritten\tmax\terased\tmax\n");
++
++	if (len >= off) {
++		if (len > count + off) len = count + off;
++		memcpy(buffer, line + off, len - off);
++		buffer += len - off;
++	}
++
++        if ((fs = get_fs_type("jffs2")) != NULL) {
++            list_for_each(p, &fs->fs_supers) {
++                sb = list_entry(p, struct super_block, s_instances);
++		c = JFFS2_SB_INFO(sb);
++
++		spin_lock(&c->erase_completion_lock);
++		jffs2_gather_stats(c, &st);
++
++		l = snprintf(line, sizeof(line),
++				"%u\t%u\t%u\t%u\t%u\t%u\t%u\t%u\t%u\t%u\t%u\t%u\t%u\t%u\t%u\t%u\n",
