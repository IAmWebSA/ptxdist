++				st.nr_n_dirent, st.nr_n_inode, st.nr_n_other,
++				st.frags_created, st.frags_obsoleted,
++				st.bytes_written, st.max_written,
++				st.blocks_erased, st.max_erased);
++		spin_unlock(&c->erase_completion_lock);
++
++		if (len + l >= off) {
++			int o = (len < off ? off - len : 0);
++			if (l > count - (len - off)) l = count - (len - off);
++			memcpy(buffer, line + o, l - o);
++			buffer += l - o;
++			if (l - o == 0) break;
++		}
++		len += l;
++            }
++        }
++
++	*eof = (len >= off + count);
++	*start = page;
++
++	return buffer - page;
++}
