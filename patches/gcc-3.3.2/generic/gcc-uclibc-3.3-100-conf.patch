   dnl Default to "generic"
   if test x$enable_clocale_flag = xno; then
     case x${target_os} in
+      xlinux-uclibc*)
+	enable_clocale_flag=uclibc
+	;;
       xlinux* | xgnu*)
 	AC_EGREP_CPP([_GLIBCPP_ok], [
         #include <features.h>
@@ -1339,6 +1342,41 @@
