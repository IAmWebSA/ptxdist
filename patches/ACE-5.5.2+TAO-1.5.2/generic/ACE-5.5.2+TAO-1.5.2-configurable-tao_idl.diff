diff -urN ACE_wrappers.orig/TAO/configure.ac ACE_wrappers/TAO/configure.ac
--- ACE_wrappers.orig/TAO/configure.ac	2006-06-10 07:05:48.000000000 +0200
+++ ACE_wrappers/TAO/configure.ac	2006-07-13 19:12:20.000000000 +0200
@@ -181,6 +181,25 @@
 TAO_ENABLE_EXAMPLES
 TAO_ENABLE_TESTS
 
+AC_MSG_CHECKING([for tao_idl command])
+AC_ARG_WITH(tao_idl,
+	AS_HELP_STRING(
+		[--with-tao_idl=PATH],
+		[Where to find a non-standard tao_idl; this option can
+		for example be used to find a build-side tao_idl while
+		cross compiling. If not set we take that one from the
+		packet.]
+	),
+	[case "${withval}" in
+	"" | y | ye | yes | n | no)
+		AC_MSG_ERROR([You must supply an argument to --with-tao_idl]) ;;
+	esac;
+	TAO_IDL="ACE_ROOT=\$(ACE_ROOT) TAO_ROOT=\$(TAO_ROOT) ${withval}"
+	],
+	TAO_IDL='ACE_ROOT=$(ACE_ROOT) TAO_ROOT=$(TAO_ROOT) $(TAO_BUILDDIR)/TAO_IDL/tao_idl'
+)
+AC_MSG_RESULT([$TAO_IDL])
+AC_SUBST(TAO_IDL)
 
 dnl
 dnl SECTION 16: AC_CONFIG_FILES([FILE...])
