From: Michael Olbrich <m.olbrich@pengutronix.de>
Date: Tue, 10 May 2016 11:45:47 +0200
Subject: [PATCH] fix parallel building

Signed-off-by: Michael Olbrich <m.olbrich@pengutronix.de>
---
 src/Makefile.in | 4 +++-
 1 file changed, 3 insertions(+), 1 deletion(-)

diff --git a/src/Makefile.in b/src/Makefile.in
index fb31707c979b..bc69f84ed412 100644
--- a/src/Makefile.in
+++ b/src/Makefile.in
@@ -116,7 +116,7 @@ ELFOBJS = @PROGRAM_ELFOBJECTS@
 
 SHELL = /bin/sh
 
-DOT_O_DEPS = sllimits.h _slang.h $(CONFIG_H)
+DOT_O_DEPS = sllimits.h _slang.h $(CONFIG_H) outputdirs
 sltoken_O_DEP = keywhash.c
 slarith_O_DEP = slarith.inc slarith2.inc
 slarrfun_O_DEP = slarrfun.inc
@@ -168,6 +168,8 @@ $(CONFIG_H) : sysconf.h Makefile
 	-$(CP) sysconf.h $(CONFIG_H)
 terminfo.inc: $(TERMINFO_INC) Makefile
 	-$(CP) $(TERMINFO_INC) terminfo.inc
+outputdirs: | $(OBJDIR) $(ELFDIR)
+.INTERMEDIATE: outputdirs
 #---------------------------------------------------------------------------
 # Intallation rules
 #---------------------------------------------------------------------------
