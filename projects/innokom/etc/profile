export PATH="$PATH:/usr/bin:/bin:/sbin:."
export LD_LIBRARY_PATH="${LD_LIBRARY_PATH:-/lib:/usr/lib}:/opt/auerswald/lib:."

#umask 022

# This fixes the backspace when telnetting in.
if [ "$TERM" != "linux" ]; then
        stty erase ^H
fi

#PS1="# "

