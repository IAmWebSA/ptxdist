export PATH="$PATH:/usr/bin:/bin:/sbin:/usr/X11R6/bin"
export DISPLAY=:0.0

#umask 022

# This fixes the backspace when telnetting in.
if [ "$TERM" != "linux" ]; then
        stty erase ^H
fi

PS1='EFCO> '
PS2='> '
PS4='+ '

alias ll='ls -l'

