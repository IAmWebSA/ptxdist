menuconfig DOTGNU
	bool "DotGNU              "
		
config TREECC
	bool
	depends on DOTGNU && BROKEN
	prompt "treecc"

comment "treecc is broken, please send a patch"
	depends on !BROKEN

config PNET
	bool
	prompt "pnet"
	select TREECC
	depends on DOTGNU
	
config PNETLIB
	bool
	prompt "pnetlib"
	select PNET
	depends on DOTGNU
