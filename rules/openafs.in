menu    "OpenAFS             "

config OPENAFS
	bool
	depends on BROKEN
	prompt "OpenAFS"

comment "openafs is broken, please send a patch"
	depends on !BROKEN

choice
	prompt "OpenAFS Version"
		default OPENAFS_1_2_11 

	config OPENAFS_1_2_11
		depends on OPENAFS
		bool
		prompt "1.2.11"

endchoice

endmenu
