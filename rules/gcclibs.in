#
# rules/gcclibs.in
#

menu	"GCC Libraries       "
	depends on BUILD_USERLAND

config GCCLIBS_CXX
	bool "libstdc++"
	select GCCLIBS_GCC_S
	select GCCLIBS_GCC_S_NOF if ARCH_PPC
	select GLIBC_LIBM
	default n

config GCCLIBS_GCC_S
	bool "libgcc_s"
	default n

config GCCLIBS_GCC_S_NOF
	bool "libgcc_s_nof"
	default n
	depends on ARCH_PPC

endmenu
