#
# rules/gcclibs.in
#

menu	"GCC Libraries       "

config GCCLIBS_CXX
	bool "libstdc++"
	select GCCLIBS_GCC_S
	select GCCLIBS_GCC_S_NOF if OPT_PPC405
	select GLIBC_LIBM
	default n

config GCCLIBS_GCC_S
	bool "libgcc_s"
	default n

config GCCLIBS_GCC_S_NOF
	bool "libgcc_s_nof"
	default n
	depends on OPT_PPC405

endmenu
