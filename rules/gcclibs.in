#
# rules/gcclibs.in
#

menu "gcc libraries                 "

config GCCLIBS
	bool

config GCCLIBS_CXX
	bool "libstdc++"
	select GCCLIBS
	select GCCLIBS_GCC_S
	select LIBC_M
	help
	  Selects the GNU Standard C++ Library
	  libstdc++ in addition to libgcc_s

config GCCLIBS_GCC_S
	bool "libgcc_s"
	select GCCLIBS
	help
	  Selects the GNU Standard C++ Library
	  libgcc_s

endmenu
