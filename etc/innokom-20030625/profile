export PATH="$PATH:/usr/bin:/bin:/sbin"

# FIXME: is this intended?? [RS]
export LD_LIBRARY_PATH="$LD_LIBRARY_PATH:/lib:/usr/lib"

#umask 022

# This fixes the backspace when telnetting in and for pdksh.
if [ "$TERM" != "linux" ]; then
        stty erase ^H
fi

