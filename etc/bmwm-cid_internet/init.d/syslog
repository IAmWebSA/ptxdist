#!/bin/sh
#
# /etc/init.d/syslog
#
# Configuration for BMW
#
# 2004/02/27 Robert Schwebel <r.schwebel@pengutronix.de>
#

case $1 in

	start)
		echo "starting syslogd:"
		/sbin/syslogd
		echo "done."
		;;
	stop)
		echo "stopping syslogd:"
		kill `/bin/pidof syslogd`
		echo "done."
		;;
	*)
		echo "usage: $0 [start|stop]"
		;;
esac

