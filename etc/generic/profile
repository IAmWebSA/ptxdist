#
# /etc/profile
#
# $Id: profile,v 1.4 2004/08/23 08:10:43 rsc Exp $
#

export PATH="$PATH:/usr/bin:/bin:/sbin"

#umask 022

# This fixes the backspace when telnetting in.
if [ "$TERM" != "linux" ]; then
        stty erase ^H
fi

PS1=@PS1@
PS2=@PS2@
PS4=@PS4@

alias ll='ls -l'
alias ..='cd ..'
alias ...='cd ../..'
alias md='mkdir'
alias rd='rmdir'
