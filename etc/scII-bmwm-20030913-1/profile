#
# /etc/profile
#
# Generic x86 configuration
#
# 2003/08/15 Robert Schwebel <r.schwebel@pengutronix.de>
#

export PATH="$PATH:/usr/bin:/bin:/sbin:/usr/X11R6/bin"
export DISPLAY=:0.0

#umask 022

# This fixes the backspace when telnetting in.
if [ "$TERM" != "linux" ]; then
        stty erase ^H
fi

# PS1="# "

